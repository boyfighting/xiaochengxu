/**index.wxss**/
page{
  background-color:#f0eff4;
  height:100%;
  overflow: scroll;
}
.twentyfour {
  font-size: 24rpx;
}
swiper-item image {
  width: 100%;
  height: 320rpx;
}
.playList .slider {
   padding: 0 24rpx; 
}
.noPhoto {
  padding: 24rpx;
  background: #fff;
  height: auto;
  /* border-bottom: 1px solid #e2e1e6; */
  box-sizing: border-box;
  display: flex; 
}

.newsPic {
  width: 68rpx;
  height: 84rpx;
  background: #f8f8f8;
  float: left;
  margin-right: 24rpx;
}
.newsTitle {
  font-size: 32rpx;
   margin-top: -8rpx; 
}
.newsInfo{
  font-size: 24rpx;
  margin-top: 20rpx;
  color: #898989;
}
.newsInfo .newsFrom {
  margin-right: 20rpx;
}

.onePhoto {
  padding: 24rpx;
  height: auto;
  display: flex;
  background: #fff;
}
.onePhotoLeft {
  width: 448rpx;
  position: relative;
  margin-left: 24rpx;
}
.onePhotoTitle {
  font-size: 32rpx;
  position: absolute;
  top:0; 
}

.onePhotoInfo {
  margin-top: 20rpx;
  color: #888;
  position: absolute;
  bottom: 0; 
}

.onePhotoInfo .onePhotoFrom {
  margin-right: 20rpx;
}

.onePhotoPic {
  width: 208rpx;
  height: 148rpx;
  background: #f8f8f8;
}

.twoPhoto {
  height: auto;
  padding: 24rpx;
  background: #fff;
}

.twoPhotoTitle {
  font-size: 32rpx;
}
.twoPhotoPic image {
  width: 50%;
  height: 212rpx;
  background: #f8f8f8;
  border-left: 1px solid #fff;
  box-sizing: border-box;
  margin-top: 10rpx;
}
.twoPhotoInfo {
  margin-top: 20rpx;
  color: #898989;
}
.twoPhotoInfo .twoPhotoFrom {
  margin-right: 20rpx;
}

.indexvideo {
  height: auto;
  padding: 24rpx;
  border-bottom: 1px solid #e2e1e6;
  box-sizing: border-box;
  background: #fff;
}

.indexvideoTitle {
  font-size: 32rpx;
  margin-bottom: 24rpx;
}
.indexvideoPic video {
  width: 100%;
  background: #f8f8f8;
  margin-top: 10rpx;
}
.indexvideoInfo {
  font-size: 24rpx;
  margin-top: 10rpx;
  color: #888;
}
.indexvideoInfo .indexvideoFrom {
  margin-right: 20rpx;
}
.threePhoto {
  height: auto;
  padding: 24rpx;
  background: #fff;
  margin-bottom: 24rpx;
}
.threePhotoPic image {
  width: 33.3%;
  height: 142rpx;
  background: #f8f8f8;
  border-left: 1px solid #fff;
  box-sizing: border-box;
  margin-top: 10rpx;
}

/*  加载更多   */
.weui-loading {
  margin: 0 5px;
  width: 20px;
  height: 20px;
  display: inline-block;
  vertical-align: middle;
  -webkit-animation: weuiLoading 1s steps(12, end) infinite;
  animation: weuiLoading 1s steps(12, end) infinite;
  background: #888;
  background-size: 100%;
}
.weui-loadmore {
  width: 65%;
  margin: 1.5em auto;
  line-height: 1.6em;
  font-size: 14px;
  text-align: center;
}
.weui-loadmore__tips {
  display: inline-block;
  vertical-align: middle;
}
.playIcon {
    display: inline-block;
    width: 60px;
    height: 60px;
    background-image: url(data:image/png;base64,);
    background-size: contain;
    position: absolute;
    left: 50%;
    top:44%;
    margin-left: -30px;
    margin-top: -30px;
}

.playvideoIcon {
  width: 92rpx;
  height: 92rpx;
  border-radius: 50%;
  background: #fff;
  /* margin-top: 10rpx; */
  position: absolute;
  top: 50%;
  left: 50%;
  margin-top: -46rpx;
  margin-left: -46rpx;
}

.playvideoIcon image {
  height: 68rpx;
  width: 92rpx;
  border-radius: 50%;
  margin-top:12rpx;
   
}

.PublishTime {
  padding: 20rpx 0;
  font-size: 24rpx;
  text-align: center;
  color: #898989;
}